{
  "name": "askray",
  "version": "1.0.0",
  "main": "app.js",
  "author": "Peter Richards",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-actuator": "^1.8.4",
    "google-auth-library": "^8.8.0",
    "jsonwebtoken": "9.0.0",
    "jwk-to-pem": "2.0.5",
    "dotenv": "^16.1.3",
    "dotenv-expand": "^10.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-actuator": "^1.8.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/node": "^20.3.1",
    "@types/node-fetch": "^2.6.4",
    "concurrently": "^8.1.0",
    "nodemon": "^2.0.22",
    "typescript": "^5.1.3"
  },
  "scripts": {
    "start": "node -r dotenv/config ./app.js dotenv_config_path=./.env.production",
    "build": "tsc",
    "start:dev": "concurrently \"tsc -w\" \"nodemon -r dotenv/config dist/app.js dotenv_config_path=.env.development\"",
    "start:prod": "concurrently \"tsc -w\" \"nodemon -r dotenv/config dist/app.js dotenv_config_path=.env.production\""
  }
}
